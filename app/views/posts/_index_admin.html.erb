<!-- Main Post Content -->
<% @post_all.each do |post| %>
  <%= render 'post_admin', :post => post %>
  <% if !(post.equal? @post_all.last) %>
    <hr>
  <% end %>
<% end %>

<!-- Pagination -->
<%= will_paginate @post_all, {:inner_window => 3, :outer_window => 0, renderer: BootstrapPagination::Rails} %>